# c.programming
//write the c program to calculate  the sum of the elements in the arrey 
#include <stdio.h>
  void main ()
{
int i,c=0,n,a[i];
   printf("enter the number of elements in the arrey :");
   scanf("%d",&n);
     for (i=0;i<n;i++)
     {
        printf("\n enter the valu of a[%d]:",i);
         scanf("%d",&a[i]);
         c=c+a[i];
      }  
        printf("\n sum of the arrey is :%d ",c);
  }
 **********************************************************************
output:
 enter the number of elements in the arrey :6
 enter the valu of a[0]:4
 enter the valu of a[1]:3
 enter the valu of a[2]:2
 enter the valu of a[3]:5
 enter the valu of a[4]:1
 enter the valu of a[5]:6
 sum of the arrey is :21
**********************************************************************
