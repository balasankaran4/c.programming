//low

// sum of the natural numbers
#include<stdio.h>
 void main ()
 {
   int n,sum=0;
      printf ("Enter the number:");
      scanf("%d",&n);
       for (int i=1;i<=n;i++)
       {
         sum=sum+i;
       }
      printf("\nSum of the natural number is %d",sum);
}
-----------------------------------------------------------------------------------------
//medium

//factorial of n 
#include<stdio.h>
 int main ()
 {
   int i,n,fact=1;
      printf ("Enter the number:");
      scanf("%d",&n);
       for (i=1;i<=n;i++)
       {
         fact=fact*i;
       }
      printf("\nThe factorial of thr number %d",fact);
     return 0;
}
-----------------------------------------------------------------------------------------
