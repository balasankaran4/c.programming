// call by value by without returntype and with parameters 
  #include <stdio.h>
void swap (int a,int b)
        {
          int temp;
          temp= a;
          a=b;
          b= temp;
        }
    void main()
    {
       int a=10,b=20;
       printf("before slapping a=%d, b=%d ",a,b);
       swap(a,b);
       printf("\nafter swapping a=%d, b=%d ",a,b);
    }
        
**********************************************************************
output
before slapping a=10, b=20
after swapping a=10, b=20
**********************************************************************
