//5*5 pattern code
#include<stdio.h>
  
      int main()
  {
       
     for (int row=1;row<=5;row++)
     {
            for (int col=1;col<=5;col++)
        {
         printf("*");
     
     }
           printf("\n");
  }
  return 0;
}
------------------------------------------------------------------------------------------
Output
*****
*****
*****
*****
*****
------------------------------------------------------------------------------------------
//integer pattern code
#include<stdio.h>
  
      int main()
  {
       
     for (int row=5;row>=1;row--)
     {
            for (int col=1;col<=row;col++)
    {
         printf("%d",col);
     
     }
           printf("\n");
  }
  return 0;
}
-----------------------------------------------------------------------------------------------
Output

12345
1234
123
12
1
