//low
 // with retuntype and parameter
  #include <stdio.h>
  int rect(int);
   int fact=1; 
   void main()
   {
   int a;
   printf ("Enter the value :");
   scanf("%d",&a);
   rect(a);
   printf("\nThe  factorial :%d",fact);
     
   }
   int rect(int x)
  {
    
   for(int i=1;i<=x;i++)
    fact=fact*i;
    return fact;
  } 
