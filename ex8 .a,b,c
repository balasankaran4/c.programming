//high
// using recursion to find the sum of the natural number
 #include<stdio.h>
int recursion(int x)
 { 
   if (x==0)
   return 0;
   else
   {
      return x+recursion(x-1);
   }
     
 }
 void main()
 {
  int n;
  printf("enter n");
  scanf("%d",&n);
  printf("%d",recursion(n));
 }
