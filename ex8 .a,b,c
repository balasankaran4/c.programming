//medium
//function quare of the elements in thwe array 
  #include<stdio.h>
  void ret();
  int i,arr[10],squ[10],n;
  void main ()
   {
   
   printf ("Enter the number of elementes in the array:");
   scanf("%d",&n);
   ret(n);
   }
   void ret (int x)
   { 
     
       for(i=0;i<x;i++)
      {
       printf ("\nEnter the element arr[%d]:",i);
       scanf("%d",&arr[i]);
       squ[i]=arr[i]*arr[i];
       }
       printf("The square of the array is:")
      for(i=0;i<x;i++)
      printf("%d ",squ[i]);
      
}
