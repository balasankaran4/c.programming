//medium
#include<stdio.h>
int main ()
{
    char ch;
    printf("Enter the character:\n");
    scanf("%c",&ch);
     
    if (ch>='a' && ch<='z')
    switch(ch)
    {
        case'a':{
             printf("vowei");}
         break;
        case'e':{
          printf("vowei");}
         break;
        case'i':{
         printf("vowei");}
         break;
        case'o':{
             printf("vowei");}
         break;
        case'u':{
            printf("vowei");}
        break;
        default:
        {
            printf("consonant");
        }    
    }
    else{
    printf("lowercase only");}
    return 0;
}


