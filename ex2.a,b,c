//high
// finding the leap year 
#include<stdio.h>
   void main ()
  {
      int year;
      
      printf ("Enter year:");
      scanf("%d",&year);
 
      
           if( year%4==0 && year%100!=0 || year%400==0 )
       {  
         printf("it is leapo year"); 
       }
        else  
        { 
          printf("not a leap year");
        }
}


